<% @users.each do |user| %>
<div class="user">
  <div class="profile-image">
  <img src=<%= user.Photo%> alt=""/>
  </div>
  <div class="user-info">
    <%= link_to "#{user.Name}", "/users/#{user.id}", class: "fs-5" %>
    <div class="num-posts">
      <p>Number of posts: <%= !user.PostsCounter ? 0 : user.PostsCounter %> </p>
    </div>
  </div>
</div>
<% end %>
<p>Hello world</p>

  